            "types": "./dist/index.d.ts",
            "default": "./dist/index.js"
        },
        "./subjects": {
            "types": "./dist/subjects/index.d.ts",
            "default": "./dist/subjects/index.js"
        },
        "./schemas": {
            "types": "./dist/schemas/index.d.ts",
            "default": "./dist/schemas/index.js"
        },
        "./types": {
